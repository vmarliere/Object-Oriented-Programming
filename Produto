
package aula20240912n;

public class Produto {
    
    public int codigo;
    public String nome;
    public int quantidade;
    
    public Produto() {
        
    }

    public Produto(int codigo, int quantidade) {
        this.codigo = codigo;
        this.nome = nome;
        this.quantidade = quantidade;
    }
    
    
    
    
}
